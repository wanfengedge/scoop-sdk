{
    "description": "BellSoft Liberica is a 100% open-source Java implementation",
    "homepage": "https://bell-sw.com/java",
    "version": "24.0.2-12",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.bell-sw.com/java/24.0.2+12/bellsoft-jdk24.0.2+12-windows-amd64.zip",
            "hash": "sha1:1221e998f013842382fee1b430934300ea6b2d19"
        }
    },
    "extract_dir": "jdk-24.0.2",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.bell-sw.com/v1/liberica/releases?bundle-type=jdk&version-modifier=latest&release-type=all&os=windows&arch=x86&installation-type=archive&package-type=zip&output=json&fields=version",
        "jsonpath": "$.version",
        "regex": "(?<major>[\\d.]+)(?:\\+)(?<build>[\\d]+)",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.bell-sw.com/java/$matchMajor+$matchBuild/bellsoft-jdk$matchMajor%2B$matchBuild-windows-amd64.zip"
            }
        },
        "hash": {
            "url": "https://api.bell-sw.com/v1/liberica/releases/$basename",
            "jsonpath": "$.sha1"
        },
        "extract_dir": "jdk-$matchMajor"
    }
}
