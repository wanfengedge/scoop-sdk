        "replace": "${dragon}-${jdk}${b}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/dragonwell-project/dragonwell21/releases/download/dragonwell-extended-$matchDragon_jdk-$matchJdk-ga/Alibaba_Dragonwell_Extended_$matchVersion_x64_windows.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "regex": "$sha256"
